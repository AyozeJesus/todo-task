  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "start-server-and-test dev http://localhost:5173 'cypress open'",
    "cypress:run": "start-server-and-test dev http://localhost:5173 'cypress run'",
    "cypress:open-only": "cypress open",
    "cypress:run-only": "cypress run",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-all": "npm run lint && npm run format:check && npm run test && npm run cypress:run",
    "check-quick": "npm run lint && npm run format:check && npm run test"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint__js": "^8.42.3",
    "@types/jquery": "^3.5.18",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.0",
    "cypress": "^13.6.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jiti": "^2.4.2",
    "jsdom": "^24.0.0",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.12",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.35.1",
    "vite": "^5.2.0",
    "vitest": "^1.5.0"
  }
